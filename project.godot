; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "GBoard",
"class": "Board",
"language": "GDScript",
"path": "res://Game/Board/Board.gd"
}, {
"base": "Node2D",
"class": "GBoard",
"language": "GDScript",
"path": "res://BoardEngine/GBoard/GBoard.gd"
}, {
"base": "GMovement",
"class": "GBoxMovement",
"language": "GDScript",
"path": "res://BoardEngine/GMovement/GBoxMovement/GBoxMovement.gd"
}, {
"base": "Node",
"class": "GMovement",
"language": "GDScript",
"path": "res://BoardEngine/GMovement/GMovement.gd"
}, {
"base": "Node2D",
"class": "GPiece",
"language": "GDScript",
"path": "res://BoardEngine/GPiece/GPiece.gd"
}, {
"base": "GPiece",
"class": "GPlayer",
